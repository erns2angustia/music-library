  "bugs": {
    "url": "https://github.com/erns2angustia/music-library/issues"
  },
  "homepage": "https://github.com/erns2angustia/music-library#readme",
  "devDependencies": {
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "eslint": "^8.35.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.9.0",
    "postgres-migrations": "^5.3.0"
  },
  "engines": {
    "node": ">=14 < 15"
